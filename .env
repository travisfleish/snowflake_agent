# Snowflake connection parameters
SNOWFLAKE_ACCOUNT=your_account_identifier
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
SNOWFLAKE_WAREHOUSE=your_warehouse
SNOWFLAKE_ROLE=your_role

# Optional Snowflake parameters (depending on your setup)
SNOWFLAKE_REGION=your_region  # If not included in account identifier
SNOWFLAKE_PRIVATE_KEY_PATH=/path/to/key.p8  # If using key pair authentication
SNOWFLAKE_PRIVATE_KEY_PASSPHRASE=your_passphrase  # If key is encrypted

# OpenAI API configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_ORGANIZATION=your_org_id  # Optional

# CrewAI configuration
CREWAI_API_KEY=your_crewai_api_key  # If using CrewAI API

# Optional - LLM configuration
MODEL_NAME=gpt-4-turbo  # Your preferred model
MAX_TOKENS=4096
TEMPERATURE=0.7